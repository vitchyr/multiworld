    </visual> 
  </link>
    
  <joint name="right_electric_gripper_base" type="fixed">
    <!-- <xacro:insert_block name="joint_origin" /> -->
    <origin rpy="0 0 0" xyz="0 0 0.1"/>
    <parent link="right_electric_gripper_base"/>
